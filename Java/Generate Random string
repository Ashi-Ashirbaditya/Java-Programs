import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        System.out.print("Enter String length : ");
        int n = sc.nextInt();
        String str = "";
        for(int i = 0; i < n; i++){
        char ch = (char) (((Math.random()*26)+65));
        str += ch;
        }
        System.out.println(str);
  }
}
